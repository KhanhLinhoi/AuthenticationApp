<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- Background Circle -->
    <path
        android:pathData="M50,50m-45,0a45,45 0,1 1,90 0a45,45 0,1 1,-90 0"
        android:fillColor="#667EEA" />

    <!-- Inner Circle -->
    <path
        android:pathData="M50,50m-35,0a35,35 0,1 1,70 0a35,35 0,1 1,-70 0"
        android:fillColor="#5A67D8" />

    <!-- Lock Icon -->
    <group android:translateX="50" android:translateY="50">
        <!-- Lock Body -->
        <path
            android:pathData="M-8,-4 L8,-4 L8,12 L-8,12 Z"
            android:fillColor="#FFFFFF"
            android:strokeColor="#FFFFFF"
            android:strokeWidth="1" />

        <!-- Lock Shackle -->
        <path
            android:pathData="M-6,-4 L-6,-8 Q-6,-12 0,-12 Q6,-12 6,-8 L6,-4"
            android:fillColor="none"
            android:strokeColor="#FFFFFF"
            android:strokeWidth="2"
            android:strokeLineCap="round" />

        <!-- Lock Dot -->
        <circle
            android:cx="0"
            android:cy="2"
            android:radius="2"
            android:fillColor="#5A67D8" />
    </group>

</vector>